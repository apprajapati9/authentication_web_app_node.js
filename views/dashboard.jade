extends base

block vars
    - var title = 'Dashboard'

block body
    h1 Dashboard
    p.
        Welcome to your dashboard! You are logged In.
    #container.col
    h2 User Information
    p First Name : #{user.firstName}
    p Last Name : #{user.lastName}
    p Email : #{user.email}
    
    p.
        <a href="\logout">LogOut! </a>